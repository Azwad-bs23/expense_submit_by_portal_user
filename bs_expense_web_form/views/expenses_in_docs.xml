<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="portal_my_home_expense" name="Show Expense" inherit_id="portal.portal_my_home" customize_show="True" priority="31">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <br/>
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">My Expenses</t>
                <t t-set="url" t-value="'/my_expenses'"/>
                <t t-set="placeholder_count" t-value="'expense_count'"/>
            </t>
        </xpath>
    </template>
</odoo>
